# Source this file to bring function into environment
#

# Avoid double inclusion
if [ "${usageImported}X" != "X" ]; then
    return 0
fi
usageImported=1

# -------------------------------------------------------
#
# Output usage.
#
usage()
{
    # output prolog (up to first line that doesn't begin with #)
    /usr/bin/sed "/^[^#]*$/q" $0
}

# -----------------------------------------------------------
